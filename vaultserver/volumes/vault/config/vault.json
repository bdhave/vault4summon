{
  "backend": {
    "file": {
      "path": "/vault/file"
    }
  },

  "listener": {
    "tcp":{
      "address": "[::]:8200",
      "tls_disable": 1
    }
  },

  "api_addr":"http://vault:8200",

  "ui": true,

  "disable_sealwrap":true,

  "seal" : {
    "transit" : {
      "address" : "http://vault:8200",
      "disable_renewal" : "false",
      "key_name": "autounseal",
      "mount_path" : "transit/",
      "tls_skip_verify" : "true"
    }
  }
}